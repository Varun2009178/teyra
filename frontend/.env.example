# Database configuration
# Replace with your own Neon database credentials
DATABASE_URL="postgresql://user:password@hostname/database?sslmode=require&channel_binding=require"
DIRECT_URL="postgresql://user:password@hostname/database?sslmode=require&channel_binding=require"

# Resend email service
RESEND_API_KEY=your_resend_api_key_here
NEXT_PUBLIC_APP_URL=https://your-app-url.com

# Google authentication (optional)
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Groq API (optional)
NEXT_PUBLIC_GROQ_API_KEY=your_groq_api_key_here

# Clerk authentication
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=your_clerk_publishable_key_here
CLERK_SECRET_KEY=your_clerk_secret_key_here

# Webhooks
CLERK_WEBHOOK_SECRET=your_clerk_webhook_secret_here